{{#if resume.education.length}}
<section id="education">
    <h2>Education</h2>
    {{#each resume.education}}
    {{#> item title=institution subtitle=(concat_subtitle studyType score area) location=city startDate=startDate endDate=endDate}}
    </ul>
    {{#if courses.length}}
    <ul class="courses">
        {{#each courses}}
        <li>{{.}}</li>
        {{/each}}
    </ul>
    {{/if}}
    {{/item}}
    {{/each}}
</section>
{{/if}}
